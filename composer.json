    "require": {
        "php": "^8.2",
        "avegacms/avegacms": "dev-master",
        "codeigniter4/framework": "^4.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.5.16"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
          "AvegaCmsBlog\\": "src/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    }
}
